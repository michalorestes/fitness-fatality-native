<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools" android:id="@+id/main__navigation"
            app:startDestination="@id/navigation_my_workouts">
    <fragment android:id="@+id/navigation_my_workouts" android:name="com.example.fitnessfatality.MyWorkoutsFragment"
              android:label="My Workouts" tools:layout="@layout/fragment_my_workouts">
        <action android:id="@+id/navigation_my_workouts" app:destination="@id/navigation_my_workouts"/>
        <action android:id="@+id/action_fragment_my_workouts_to_navigation_my_profile"
                app:destination="@id/navigation_my_profile"/>
        <action android:id="@+id/action_fragment_my_workouts_to_navigation_my_settings"
                app:destination="@id/navigation_my_settings"/>
        <action android:id="@+id/action_navigation_my_workouts_to_mainActivityOld"
                app:destination="@id/mainActivityOld" app:popExitAnim="@anim/nav_default_pop_exit_anim"
                app:popEnterAnim="@anim/nav_default_pop_enter_anim" app:exitAnim="@anim/nav_default_exit_anim"
                app:enterAnim="@anim/nav_default_enter_anim"/>
    </fragment>
    <fragment android:id="@+id/navigation_my_profile" android:name="com.example.fitnessfatality.MyProfileFragment"
              android:label="My Profile" tools:layout="@layout/fragment_my_profile"/>
    <fragment android:id="@+id/navigation_my_settings" android:name="com.example.fitnessfatality.MyStatisticsFragment"
              android:label="Settins" tools:layout="@layout/fragment_my_statistics"/>
    <activity android:id="@+id/mainActivityOld" android:name="com.example.fitnessfatality.MainActivityOld"
              android:label="old_activity_main" tools:layout="@layout/old_activity_main"/>
</navigation>